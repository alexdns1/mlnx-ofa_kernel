 	struct bio_vec		inline_bvec[NVMET_MAX_INLINE_BIOVEC];
 	union {
 		struct {
@@ -399,7 +407,9 @@ struct nvmet_req {
 		} b;
 		struct {
 			bool			mpool_alloc;
