 	sysfs_remove_group(&dev->dev.kobj, &debug_group);
 	sysfs_remove_group(&dev->dev.kobj, &settings_group);
@@ -1169,7 +1206,7 @@ void mlx5e_sysfs_remove(struct net_devic
 	priv->ecn_root_kobj = NULL;
 }
 
